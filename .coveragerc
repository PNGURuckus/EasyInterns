[run]
include =
    app/models.py
    app/scrapers/base.py
    app/scrapers/indeed.py
    app/scrapers/talent.py
    app/config.py
    app/core/auth.py
    app/database.py
    main.py
omit =
    app/api.py
    app/api_routes/*
    app/auth.py
    app/cli.py
    app/core/ai.py
    app/core/email_utils.py
    app/db.py
    app/email_harvester.py
    app/pipeline.py
    app/ranking/*
    app/repositories.py
    app/resume/*
    app/schemas.py
    app/scoring.py
    app/scrapers/bcps.py
    app/scrapers/greenhouse.py
    app/scrapers/job_bank.py
    app/scrapers/lever.py
    app/scrapers/ops.py
    app/scrapers/registry.py
    app/scrapers/rss.py
    app/utils.py
    app/workers/*

[report]
show_missing = True
